      <script>out.println("ALERT");
com.diplab.serializableDevice.SerializableDevice131.getInstanceOfSerializableDevice131().alerton();</script>
    </scriptTask>
    <sequenceFlow id="flow7" name="avgco2&gt;1000 || avgco&gt;200" sourceRef="exclusivegateway1" targetRef="scripttask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${avgco2>1000 || avgco>200}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="scripttask3" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="endevent1"></sequenceFlow>
